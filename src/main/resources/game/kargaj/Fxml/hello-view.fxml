<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="341.0" prefWidth="375.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.kargaj.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TextField fx:id="input" />
      <Button fx:id="btn1" mnemonicParsing="false" onAction="#btn1OnClick" prefHeight="25.0" prefWidth="161.0" text="NewGame" />
      <Button fx:id="btn2" mnemonicParsing="false" prefHeight="25.0" prefWidth="165.0" text="Login" />
      <Button fx:id="btn3" mnemonicParsing="false" prefHeight="25.0" prefWidth="170.0" text="game" />
   </children>
</VBox>
